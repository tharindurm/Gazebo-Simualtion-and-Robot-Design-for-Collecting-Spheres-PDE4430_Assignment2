            <geometry>
                <box size="0.001 0.001 0.001" />
            </geometry>
        </visual>
    </link>
    -->
    <link name="base_link">
        <xacro:Rectangle_Panel x="0.0" y="0.175" z="0.215" roll="0.0" pitch="0.0" yaw="0"/>
        <xacro:Rectangle_Panel x="0.0" y="-0.175" z="0.215" roll="0.0" pitch="0.0" yaw="0"/>
        <xacro:Rectangle_Panel x="-0.175" y="0.0" z="0.215" roll="0.0" pitch="0.0" yaw="1.5708"/>
    </link>

    
    <xacro:Wheel prefix="right" x="0.075" y="-0.200" z="0.125" roll="1.5708" pitch="0.0" yaw="0"/>
    <xacro:Wheel prefix="left" x="0.075" y="0.200" z="0.125" roll="1.5708" pitch="0.0" yaw="0"/>



    


    <joint name = "right_wheel_joint"  type = "continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0.200 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" rpy="0 0 0" />
    </joint>
    <joint name = "left_wheel_joint"  type = "continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0.200 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" rpy="0 0 0" />
    </joint>
