<?xml version="1.0"?>
<robot name="myrobot" xmlns:xacro = "http://www.ros.org/wiki/xacro">

    
    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="green">
        <color rgba="0 0.8 0 1"/>
    </material>

    <material name="red">
        <color rgba="0.8 0 0 1"/>
    </material>


    <xacro:macro name="box_inertia" params="m w h d">
        <inertial>
            <mass value="${m}"/>
            <inertia ixx="${m / 12.0 * (d*d + h*h)}" ixy="0.0" ixz="0.0" iyy="${m / 12.0 * (w*w + h*h)}" iyz="0.0" izz="${m / 12.0 * (w*w + d*d)}"/>
            </inertial>
    </xacro:macro>

    <xacro:macro name="Rectangle_Panel" params="x y z roll pitch yaw">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="3.978"/>
            <inertia ixx="0.062083" ixy="0.0" ixz="0.0"
                     iyy="0.020833" iyz="-0.0"
                     izz="0.042083"/>
        </inertial>
        <visual>
            <origin xyz="${x} ${y} ${z}" rpy="${roll} ${pitch} ${yaw}" />
            <geometry>
                <box size="0.35 0.02 0.35" />
            </geometry>
            <material name="blue" />    
        </visual>
        <collision>
            <origin xyz="${x} ${y} ${z}" rpy="${roll} ${pitch} ${yaw}" />
            <geometry>
                <box size="0.35 0.02 0.35" />
            </geometry>
            <material name="blue" />
        </collision>
    </xacro:macro>

    <xacro:macro name="Wheel" params="prefix x y z roll pitch yaw">
        <link name="${prefix}_wheel">
            <visual>
                <geometry>
                    <cylinder length="0.03" radius="0.125"/>
                </geometry>
                <origin xyz="${x} ${y} ${z}" rpy="${roll} ${pitch} ${yaw}"/>
                <material name="green"/>
            </visual>
            <collision>
                <origin xyz="${x} ${y} ${z}" rpy="${roll} ${pitch} ${yaw}" />
                <geometry>
                    <cylinder length="0.03" radius="0.125"/>
                </geometry>
                <material name="green" />
            </collision>
            <inertial>
                <origin xyz="0 0 0" rpy="0 0 0"/>
                <mass value="3.978"/>
                <inertia ixx="0.021858680" ixy="0.0" ixz="0.010020353"
                        iyy="0.078572349" iyz="-0.0"
                        izz="0.091971324"/>
            </inertial>
        </link>
    </xacro:macro>



    <xacro:property name="DoorWidth" value="0.20" />
    <xacro:property name="DoorThickness" value="0.03" />
    <xacro:property name="DoorHeight" value="0.2" />

    <xacro:macro name="Door_Panel" params="x y z roll pitch yaw">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1.0"/>
            <inertia ixx="0.062083" ixy="0.0" ixz="0.0"
                     iyy="0.020833" iyz="-0.0"
                     izz="0.042083"/>
        </inertial>
        <visual>
            <origin xyz="${x} ${y} ${z}" rpy="${roll} ${pitch} ${yaw}" />
            <geometry>
                <box size="${DoorWidth} ${DoorThickness} ${DoorHeight}" />
            </geometry>
            <material name="blue" />    
        </visual>
        <collision>
            <origin xyz="${x} ${y} ${z}" rpy="${roll} ${pitch} ${yaw}" />
            <geometry>
                <box size="${DoorWidth} ${DoorThickness} ${DoorHeight}" />
            </geometry>
            <material name="blue" />
        </collision>
    </xacro:macro>



